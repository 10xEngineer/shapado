.tag-list
  %ul.list
    -(tag_list).each do |tag|
      -unless current_tags.include? tag["name"]
        %li
          %span.tag
            =link_to h(tag["name"]), url_for(:tags => (current_tags + [tag["name"]]).join("+")), :rel => "tag"
          .tag-multiplier
            x
            = tag["count"].to_i
          .clear
