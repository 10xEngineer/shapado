- if RAILS_ENV == "production" && !AppConfig.analytics_id.empty? && !current_group.has_custom_analytics
  %a#googlean{:href => AppConfig.analytics_id, :style => "display: none", :rel=>"nofollow" }
  =javascript_include_tag 'jquery.geekga-1.1.min.js'
  =javascript_include_tag 'modules/googlean'
-elsif RAILS_ENV == "production" && current_group.has_custom_analytics && current_group.analytics_vendor == 'googlean'
  %a#googlean{:href => current_group.analytics_id, :style => "display: none", :rel=>"nofollow" }
  =javascript_include_tag 'jquery.geekga-1.1.min.js'
  =javascript_include_tag 'modules/googlean'