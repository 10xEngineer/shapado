= stylesheet_link_tag "jquery-ui", "compiled/application"
= stylesheet_link_tag '/javascripts/openid/css/style.css', :media => 'screen, projection'

%link#current-theme{ :href => "/stylesheets/compiled/themes/#{current_group.theme}/style.css", :rel => "stylesheet", :media => "screen", :type => "text/css" }
%link#current-nav{ :href => "/stylesheets/compiled/navigation/#{current_group.theme}.css", :rel => "stylesheet", :media => "screen", :type => "text/css" }

= yield :css
