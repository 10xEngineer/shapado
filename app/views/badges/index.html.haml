-content_for :subtabs do
  .navtitle
    = t(".title")

%ul.list
  -Badge::TOKENS.each do |token|
    %li
      -badge = Badge.new(:token => token)
      = render :partial => "badges/badge", :object => badge, :locals => {:mini => true}
      %b
        == x #{Badge.count(:token => token, :group_id => current_group.id)}
